<launch>  

  <arg name="urdf_filename" default="$(find galileo_biqu_description)/biqu.urdf" />  
  <arg name="solver_id" default="biqu" />
  <arg name="problem_parameters_location" default="$(find galileo_biqu_description)/Parameters/problem_parameters.txt" />
  <arg name="robot_body_frame_name" default="base" />
  <arg name="environment_surfaces_location" default="$(find galileo_biqu_description)/Parameters/environment_surfaces/" />

  <param name="galileo_ros/urdf_filename" value="$(arg urdf_filename)"/>
  <param name="robot_description" command = "$(find xacro)/xacro --inorder $(find galileo_biqu_description)/biqu.urdf" />
  <param name="galileo_ros/solver_id" value="$(arg solver_id)"/>
  <param name="galileo_ros/problem_parameters_location" value="$(arg problem_parameters_location)"/>
  <param name="galileo_ros/robot_body_frame_name" value="$(arg robot_body_frame_name)"/>  
  <param name="galileo_ros/environment_surfaces_location" value="$(arg environment_surfaces_location)"/>

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <!-- <param name="ignore_timestamp" type="bool" value="true"/> -->
  </node>

  <node name="rviz" pkg="rviz" type="rviz" output="screen" args="-d $(find galileo_ros)/rviz/BiQu.rviz" />


  <node name="galileo_ros_legged_rviz_node" type="galileo_ros_legged_rviz_node" pkg="galileo_ros" output="screen">
  </node>


  <node name="galileo_ros_legged_verbose_rviz" type="galileo_ros_legged_verbose_rviz" pkg="galileo_ros" output="screen">  
  </node>

  
</launch>